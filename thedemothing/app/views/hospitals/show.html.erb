<%- model_class = Hospital -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize %></h1>
</div>
<h5>Hospital Name: <%= @hospital.hospital_name %></h5>
<div class="bs-docs-example">
            <div class="tabbable tabs-left">
              <ul class="nav nav-tabs">
                <li class="active"><a href="#lA" data-toggle="tab">Basic Information</a></li>
                <li><a href="#lB" data-toggle="tab">Statistics</a></li>
                <li><a href="#lC" data-toggle="tab">Doctor's Information</a></li>
                <li><a href="#lD" data-toggle="tab">Hospital Reports</a></li>
