              <div class="tab-content">
<div class="tab-pane active" id="lA">
  <div class="span8">
<table class="table table-striped table-condensed">
  <tbody
    <tr>
        <td>Hospital Name</td>
        <td><%= best_in_place @hospital, :hospital_name %><br/></td>
        </td> 
      </tr>
