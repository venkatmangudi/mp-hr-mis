<h1>Listing employment_types</h1>

<table>
  <tr>
    <th>Employment type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @employment_types.each do |employment_type| %>
  <tr>
    <td><%= employment_type.employment_type %></td>
    <td><%= link_to 'Show', employment_type %></td>
    <td><%= link_to 'Edit', edit_employment_type_path(employment_type) %></td>
    <td><%= link_to 'Destroy', employment_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

