<%- model_class = HealthDeptLocation -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:hospital_name) %>:</strong></dt>
  <dd><%= @health_dept_location.institution_master.Institution_Name%><dt>
  <dt><strong><%= model_class.human_attribute_name(:division_id) %>:</strong></dt>
  <dd><%= @health_dept_location.division.division_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:district_id) %>:</strong></dt>
  <dd><%= @health_dept_location.district.district_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:block_id) %>:</strong></dt>
  <dd><%= @health_dept_location.block.block_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:location_id) %>:</strong></dt>
  <dd><%= @health_dept_location.location.location_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:admin_location) %>:</strong></dt>
  <dd><%= @health_dept_location.admin_location %></dd>
  <dt><strong><%= model_class.human_attribute_name(:istribal) %>:</strong></dt>
  <dd><%= @health_dept_location.istribal %></dd>
  <dt><strong><%= model_class.human_attribute_name(:no_of_beds) %>:</strong></dt>
  <dd><%= @health_dept_location.no_of_beds %></dd>


<!--  <dt><strong><%= model_class.human_attribute_name(:district_code) %>:</strong></dt>
  <dd><%= @health_dept_location.district_code %></dd>-->
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              health_dept_locations_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_health_dept_location_path(@health_dept_location), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              health_dept_location_path(@health_dept_location),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
