<!DOCTYPE html>
<html>
  <head>
    <style type="text/css" media="screen">
          #mp_logo{
          	padding-left: 200px;
          	padding-right: 30px;
            padding-top: 10px;
          }

          #mp_logo {
        background-image: url("/assets/mp_logo.png");
        background-repeat: no-repeat;
        background-position: top left;
        height: 155px;
      }
    </style>
    <link rel="stylesheet" href="//cdn.oesmith.co.uk/morris-0.4.1.min.css">
<script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.0/jquery.min.js"></script>
<script src="//cdnjs.cloudflare.com/ajax/libs/raphael/2.1.0/raphael-min.js"></script>
<script src="//cdn.oesmith.co.uk/morris-0.4.1.min.js"></script>
</head>
<body>

     <div class="container">
      <div class="hero-unit">
              <div id="mp_logo">
                  <h1>
                    <strong>Human Resources - MIS</strong><br>
                  <small>Department of Public Health & Family Welfare</small>
                </h1>
            </div>
     </div>
   </div>

    <div class="row">
        <div class="span9">
          <div id="graph" style="height: 250px;"></div>
          
        </div>
        <div class="span3">

          <div id="donut-example" style="height: 250px;"></div>
       </div>
    </div>

   <script >
  Morris.Bar({
  element: 'graph',
  data: [
    {x: 'Pharmacist', y: 26},
    {x: 'Cardiothorasic', y: 14},
    {x: 'Neurosurgeon', y: 69},
    {x: 'Endocarditist', y: 23},
    {x: 'Aenesthician', y: 25},
    {x: 'Radiologist', y: 52},
  ],
  xkey: 'x',
  ykeys: ['y'],
  labels: ['Y'],
  barColors: function (row, series, type) {
    if (type === 'bar') {
      var red = Math.ceil(255 * row.y / this.ymax);
      return 'rgb(' + red + ',0,0)';
    }
    else {
      return '#000';
    }
  }
});

Morris.Donut({
  element: 'donut-example',
  data: [
    {label: "Males", value: 70},
    {label: "Females", value: 27},
    {label: "Others", value: 3}
  ]
});
   </script>
</body>
