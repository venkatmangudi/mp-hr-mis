<%= simple_form_for @special_cadre, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :Special_Cadre_Type %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                special_cadres_path, :class => 'btn' %>
  </div>
<% end %>
